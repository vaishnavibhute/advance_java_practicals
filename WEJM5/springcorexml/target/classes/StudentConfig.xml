<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspiders.springcorexml.bean.StudentBean"
		name="Student1">
		<property name="id">
			<value>1</value>
		</property>
		<property name="name" value="Amit"></property>
		<property name="email" value="Amit@gmail.com"></property>
		<property name="contact" value="1234567809"></property>
	</bean>
	
	<bean class="com.jspiders.springcorexml.bean.StudentBean"
		name="Student2">
		<property name="id" value="2"></property>
		<property name="name" value="Sumit"></property>
		<property name="email" value="Sumit@gmail.com"></property>
		<property name="contact" value="3456789120"></property>
	</bean>
	
	
	<bean class="com.jspiders.springcorexml.bean.StudentBean" name="Student3">
	<constructor-arg name="id" value="1"></constructor-arg>
	<constructor-arg name="name" value="Sumit"></constructor-arg>
	<constructor-arg name="email" value="Sumit@gmail.com"></constructor-arg>
	<constructor-arg name="contact" value="3456789120"></constructor-arg>
	</bean>
</beans>